{
    "url": "/graphql",
    "method": "POST",
    "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {{connection.apiKey}}"
    },
    "body": {
        "query": "mutation { phishingCampaignRunStop(campaignId: {{parameters.campaignId}}, runId: {{parameters.runId}}) { errors { field placeholders reason recordId } node { id campaign status } } }"
    },
    "response": {
        "output": "{{body}}"
    }
}