{
    "url": "/graphql",
    "method": "POST",
    "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {{connection.apiKey}}"
    },
    "body": {
        "query": "mutation { userArchive(userIds: [{{parameters.selectUsers}}]) { errors { field placeholders reason recordId }node {email archivable archived archivedAt employeeNumber }}}"
    },
    "response": {
        "output": "{{body}}"
    }
}